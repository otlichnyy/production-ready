  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@svgr/cli": "^6.2.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "cross-env": "^7.0.3",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  }
}
